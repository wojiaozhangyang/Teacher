<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 

    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.anweitech.teacher.domain" >

	<class name="Student" table="student" >

		<id name="sid">
			<generator class="assigned"></generator>
		</id>

		<property name="sname"  ></property>
		<property name="slogtime"  ></property>
		<property name="uuid"  ></property>
		<property name="whether_online"  ></property>
		<property name="sgroup"  ></property>
		<property name="level"  ></property>
		
	</class>

</hibernate-mapping>